{
    "name": "outeredge/magento-base-module",
    "description": "Magento 2 Base Module by outer/edge",
    "type": "magento2-module",
    "license": [
        "MIT"
    ],
    "authors": [
        {
            "name": "outer/edge",
            "email": "support@outeredge.agency",
            "homepage": "https://outeredge.agency/",
            "role": "Developer"
        }
    ],
    "require": {
        "magento/product-community-edition": ">=2.4.4",
        "outeredge/magento-limitrole-module": "^1.0",
        "outeredge/magento-layout-module": "^3.3",
        "outeredge/magento-bulk-actions": "^1.0",
        "outeredge/magento-language-en_gb-module": "^1.0",
        "open-telemetry/opentelemetry-logger-monolog": "^1.0.0"
    },
    "suggest": {
        "outeredge/magento-disable-common": "To remove commonly unused modules",
        "outeredge/magento-disable-graphql": "To remove GraphQL modules when not in use"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "OuterEdge\\Base\\": ""
        }
    }
}
